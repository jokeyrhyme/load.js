{
  "name": "@jokeyrhyme/load",
  "description": "simple script loader based on extracted logic from RequireJS",
  "version": "1.1.0",
  "author": "Ron Waldon (https://github.com/jokeyrhyme)",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-es3-member-expression-literals",
            "transform-es3-property-literals"
          ]
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/jokeyrhyme/load.js/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^16.1.1",
    "eslint": "^6.7.1",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "fixpack": "^2.3.1",
    "gulp": "^3.8",
    "tape": "^4.2",
    "vinyl-buffer": "^1.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "files": [
    "dist/loadjs.js",
    "loadjs.js",
    "LICENSE.txt",
    "README.md"
  ],
  "homepage": "https://github.com/jokeyrhyme/load.js#readme",
  "keywords": [
    "loader",
    "loadjs",
    "requirejs",
    "script"
  ],
  "license": "BSD-3-Clause",
  "main": "loadjs.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/load.js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "posttest": "npm run eslint",
    "pretest": "gulp && npm run fixpack",
    "test": "tape tests/index.js"
  }
}
