{
  "name": "@jokeyrhyme/load",
  "description": "simple script loader based on extracted logic from RequireJS",
  "version": "1.1.0",
  "author": "Ron Waldon (https://github.com/jokeyrhyme)",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "es3.memberExpressionLiterals",
            "es3.propertyLiterals"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@jokeyrhyme/eslint-config-es5": "^1.0.0",
    "babelify": "^6.3",
    "browserify": "^11.2",
    "eslint": "^1.5.1",
    "eslint-config-semistandard": "^5.0",
    "eslint-config-standard": "^4.4",
    "eslint-plugin-standard": "^1.3.1",
    "fixpack": "^2.2.0",
    "gulp": "^3.8",
    "tape": "^4.2",
    "vinyl-buffer": "^1.0",
    "vinyl-source-stream": "^1.1"
  },
  "files": [
    "dist/loadjs.js",
    "loadjs.js",
    "LICENSE.txt",
    "README.md"
  ],
  "keywords": [
    "loader",
    "loadjs",
    "requirejs",
    "script"
  ],
  "license": "BSD-3-Clause",
  "main": "loadjs.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/load.js.git"
  },
  "scripts": {
    "lint": "eslint . && fixpack",
    "posttest": "npm run lint",
    "pretest": "gulp",
    "test": "tape tests/index.js"
  }
}
