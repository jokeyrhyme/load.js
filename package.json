{
  "name": "@jokeyrhyme/load",
  "version": "1.0.0",
  "description": "simple script loader based on extracted logic from RequireJS",
  "main": "loadjs.js",
  "scripts": {
    "pretest": "gulp",
    "test": "eslint . && tape tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/load.js.git"
  },
  "author": "Ron Waldon (https://github.com/jokeyrhyme)",
  "license": "BSD-3-Clause",
  "keywords": [
    "loadjs",
    "script",
    "loader",
    "requirejs"
  ],
  "devDependencies": {
    "babelify": "^6.0",
    "browserify": "^10.2",
    "eslint": "^0.24.0",
    "eslint-config-semistandard": "^4.0",
    "eslint-config-standard": "^3.3",
    "gulp": "^3.8",
    "tape": "^4.0",
    "vinyl-buffer": "^1.0",
    "vinyl-source-stream": "^1.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "es3.memberExpressionLiterals",
            "es3.propertyLiterals"
          ]
        }
      ]
    ]
  },
  "files": [
    "dist/index.js",
    "index.js",
    "LICENSE.txt",
    "README.md"
  ]
}
